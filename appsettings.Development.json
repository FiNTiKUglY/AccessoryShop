{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Default": "Information",
        "Microsoft": "Fatal",
        "Microsoft.Hosting.Lifetime": "Fatal"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/loggingFile-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
